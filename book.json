{
    "plugins": [
        "multipart",
        "edit-link",
        "comment",
        "disqus",
        "jsfiddle"
    ],
    "pluginsConfig": {
        "edit-link": {
            "base": "https://github.com/Touna-App-Front-End/rnrstudy/edit/master",
            "label": "Edit This Page"
        },
        "comment": {
            "highlightCommented": true
        },
