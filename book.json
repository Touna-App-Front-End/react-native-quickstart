{
    "author": "SimMan <liwei0990@gmail.com>",
    "description": "React Native Quickstart",
    "gitbook": ">=2.4.0",
    "extension": null,
    "generator": "site",
    "links": {
        "sharing": {
            "all": null,
            "facebook": null,
            "google": null,
            "twitter": null,
            "weibo": null
        }
    },
    "pdf": {
        "fontSize": 12,
        "footerTemplate": null,
        "headerTemplate": null,
        "margin": {
            "bottom": 36,
            "left": 62,
            "right": 62,
            "top": 36
        },
        "pageNumbers": false,
        "paperSize": "a4"
    },
    "plugins": [
        "disqus",
        "anchors",
        "maxiang",
        "ungrey",
        "toggle-chapters",
        "codeblock-filename",
        "-sharing",
        "-highlight",
        "pygments",
        "edit-link",
        "comment",
        "jsfiddle"
    ],
    "pluginsConfig": {
        "edit-link": {
            "base": "https://github.com/Touna-App-Front-End/react-native-quickstart/edit/master",
            "label": "Edit This Page"
        },
        "comment": {
            "highlightCommented": true
        },
        "disqus": {
            "shortName": "rnrstudy",
            "useIdentifier": false
        },
        "jsfiddle": {
            "type": "script",
            "tabs": ["result", "js", "css", "html"],
            "height": "500",
            "width": "500",
            "fontColor": "00FF00"
        }
    }
}